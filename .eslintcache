multipleprops);\n    const {component: Component,auth ,...rest} = multipleprops;\n\n    return <Route\n        {...rest}\n        render = {\n            props => auth.isAuthenticated === true ? (<Component {...props} />) : (<Redirect to='/login' />)\n        }    \n    />\n};\n\nconst mapStateToProps = state =>({\n    auth: state.auth\n});\n\nexport default connect(mapStateToProps,{})(PrivateRoute);","/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/layout/Footer.js",["175"],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/auth/Register.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/profile/Profile.js",["176"],"import React, { Component } from 'react'\nimport PropTypes from 'prop-types'\nimport { connect } from 'react-redux'\nimport { Link } from 'react-router-dom'\nimport Spinner from '../common/Spinner'\nimport { getProfileByHandle } from '../../actions/profileActions'\n\nclass Profile extends Component {\n\n  componentDidMount() {\n    if (this.props.match.params) {\n      console.log(this.props.match.params.username.toString());\n      this.props.getProfileByHandle(this.props.match.params.username.toString());\n    }\n  }\n\n  render() {\n\n    const { profile, loading } = this.props.profile;\n    let profileContent;\n    console.log(profile);\n    if (profile === null || loading) {\n      profileContent = <Spinner />\n    } else {\n      profileContent = (<div className=\"profile\">\n        <div className=\"container\">\n          <div className=\"row\">\n            <div className=\"col-md-12\">\n              <div className=\"row\">\n                <div className=\"col-6\">\n                  <Link to=\"/profiles\" className=\"btn btn-light mb-3 float-left\">Back To Profiles</Link>\n                </div>\n                <div className=\"col-6\"></div>\n              </div>\n              <div className=\"row\">\n                <div className=\"col-md-12\">\n                  <div className=\"card card-body bg-light mb-3\">\n                    <div className=\"row\">\n                      <div className=\"col-4\">\n                        <img className=\"rounded-circle\" src={profile[0].avatar} />\n                      </div>\n                      <div className=\"col-4 p-5 text-left border-left\">\n                        <h3 className=\"text-info\">{profile[0].username}'s Bio</h3>\n                        <p>Name : {profile[0].firstname} {profile[0].lastname}</p>\n                        <p>Mail ID : {profile[0].email}</p>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>);\n    }\n    return (\n      <React.Fragment>\n        {profileContent}\n      </React.Fragment>\n    )\n  }\n}\n\nProfile.propTypes = {\n  profile: PropTypes.object.isRequired,\n  getProfileByHandle: PropTypes.func.isRequired\n}\n\nconst mapStateToProps = (state) => ({\n  profile: state.profile\n})\n\n\nexport default connect(mapStateToProps, { getProfileByHandle })(Profile);","/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/profiles/Profiles.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/post/Post.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/posts/Posts.js",["177","178","179"],"import React, { Component } from 'react'\nimport {connect} from 'react-redux'\nimport PropTypes from 'prop-types'\nimport PostForm from './PostForm'\nimport TextFieldGroup from '../common/TextFieldGroup'\nimport TextAreaFieldGroup from '../common/TextAreaFieldGroup'\nimport {getPosts} from '../../actions/postActions'\nimport Spinner from '../common/Spinner'\nimport PostFeed from './PostFeed'\n\nclass Posts extends Component {\n\n  componentDidMount(){\n    this.props.getPosts();\n  }\n\n  render() {\n\n    const {loading, posts} = this.props.post;\n    let postContent;\n\n    if(posts === null || loading){\n      postContent = <Spinner />\n    }\n    else{\n      postContent = <PostFeed posts={posts} />\n    }\n\n    return (\n      <div className=\"feed\">\n        <div className=\"container\">\n          <div className=\"row\">\n            <div className=\"col-md-12\">\n            <PostForm />\n            <br/>\n            {postContent}\n            </div>\n          </div>\n        </div>    \n      </div>\n    )\n  }\n}\n\nconst mapStateToProps = (state) => ({\n  post: state.post\n})\n\nexport default connect(mapStateToProps,{getPosts})(Posts);","/Users/harsh/Desktop/Hyperverge/social-media-app/src/actions/types.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/common/Spinner.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/common/TextFieldGroup.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/common/TextAreaFieldGroup.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/profiles/ProfilesItem.js",["180","181"],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/posts/PostFeed.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/actions/postActions.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/posts/PostForm.js",["182","183"],"import React, { Component } from 'react'\nimport { connect } from 'react-redux'\nimport PropTypes from 'prop-types'\nimport TextAreaFieldGroup from '../common/TextAreaFieldGroup';\nimport TextFieldGroup from '../common/TextFieldGroup';\nimport { addPost } from '../../actions/postActions'\nclass PostForm extends Component {\n\n  constructor() {\n    super();\n    this.state = {\n      title: '',\n      description: '',\n      image: null,\n      imgURL: null,\n      errors: {}\n    }\n    this.onChange = this.onChange.bind(this);\n    this.onSubmit = this.onSubmit.bind(this);\n  }\n\n  componentWillReceiveProps(newProps) {\n    if (newProps.errors) {\n      this.setState({ errors: newProps.errors })\n    }\n  }\n\n  onSubmit(e) {\n    e.preventDefault();\n    const { user } = this.props.auth;\n    let newPost = new FormData();\n    newPost.append(\"title\", this.state.title);\n    newPost.append(\"description\", this.state.description);\n    newPost.append(\"image\", this.state.image);\n    this.props.addPost(newPost);\n    this.setState({ title: '', description: '', image: null, imgURL: null });\n  }\n\n  onChange(e) {\n    e.preventDefault();\n    if (e.target.name === 'image') {\n      this.setState({ [e.target.name]: e.target.files[0], imgURL: URL.createObjectURL(e.target.files[0]) })\n    }\n    else {\n      this.setState({\n        [e.target.name]: e.target.value\n      })\n    }\n  }\n\n  render() {\n\n    const { errors } = this.state;\n    return (\n      <div className=\"post-form mb-3\">\n        <div className=\"card card-info bg-secondary\">\n          <div className=\"card-header bg-info text-white\">\n            Say Something...\n          </div>\n          <div className=\"card-body\">\n            <form>\n              <div className=\"form-group\">\n                <TextFieldGroup\n                  placeholder=\"Post Title\"\n                  name=\"title\"\n                  value={this.state.title}\n                  onChange={this.onChange}\n                  error={errors.text}\n                />\n                <TextAreaFieldGroup\n                  placeholder=\"Post Description\"\n                  name=\"description\"\n                  value={this.state.description}\n                  onChange={this.onChange}\n                  error={errors.text}\n                />\n                {this.state.imgURL ? (<img style={{ width: \"600px\", height: \"500px\" }} src={this.state.imgURL} />) : ('')}\n              </div>\n              <label for=\"image\"><i class='m-2 p-2 fas fa-camera btn btn-light text-info' style={{ cursor: \"pointer\" }}> Upload Image </i></label>\n              <input\n                type=\"file\"\n                name=\"image\"\n                id=\"image\"\n                accept=\"image/*\"\n                onChange={this.onChange}\n                style={{ display: \"none\" }}\n                error={errors.text}\n              />\n              <button onClick={this.onSubmit} className=\"btn btn-light\">Submit</button>\n            </form>\n          </div>\n        </div>\n      </div>\n    )\n  }\n}\n\nPostForm.propTypes = {\n  auth: PropTypes.object.isRequired,\n  errors: PropTypes.object.isRequired,\n  addPost: PropTypes.func.isRequired\n}\n\nconst mapStateToProps = (state) => ({\n  auth: state.auth,\n  errors: state.errors\n});\n\nexport default connect(mapStateToProps, { addPost })(PostForm);\n","/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/posts/PostItem.js",["184","185","186","187"],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/post/CommentForm.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/post/CommentFeed.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/common/SelectListGroup.js",[],["188","189"],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/common/InputGroup.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/validations/is-empty.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/reducers/index.js",[],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/components/post/CommentItem.js",["190","191"],"/Users/harsh/Desktop/Hyperverge/social-media-app/src/reducers/errorsReducer.js",["192"],"import {GET_ERRORS,CLEAR_ERRORS} from '../actions/types';\nconst initialState = {};\n\nexport default function(state=initialState,action){\n\n    switch(action.type){\n       \n        case GET_ERRORS: \n        return action.payload;\n\n        case CLEAR_ERRORS: \n        return {};\n       \n        default:\n        return state;\n    }\n}","/Users/harsh/Desktop/Hyperverge/social-media-app/src/reducers/authReducer.js",["193"],"import {TYPE_TEST,